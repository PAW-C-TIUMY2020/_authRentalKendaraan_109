@model RentalKendaraan_109.Models.Peminjaman

@{
    ViewData["Title"] = "Hapus";
}

<h2>Hapus Data Peminjaman</h2>

<h3>Apakah anda yakin ingin menghapus data ini?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.TglPeminjaman)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TglPeminjaman)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Biaya)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Biaya)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IdCustomerNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdCustomerNavigation.IdCustomer)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.IdJaminanNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IdJaminanNavigation.IdJaminan)
        </dd>
